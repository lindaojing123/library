<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="cart" extends="struts-default " namespace="/cart">
        <action name="changeCart" class="com.web.CartAction" method="changeCart">
            <result name="changeCart">/front/cart/cartList.jsp</result>
        </action>
        <action name="addCart" class="com.web.CartAction" method="addCart">
            <result name="addCart">/front/cart/cartList.jsp</result>
        </action>
        <action name="deleteCart" class="com.web.CartAction" method="deleteCart">
            <result name="deleteCart">/front/cart/cartList.jsp</result>
        </action>
    </package>
</struts>
