<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="admin" extends="struts-default" namespace="/admin">
            <action name="login" class="com.web.AdminAction" method="login">
                <result name="success">/back/main/main.jsp</result>
                <result name="error">/back/login.jsp</result>
            </action>
           <action name="exit" class="com.web.AdminAction" method="exit">
              <result name="success">/back/login.jsp</result>
           </action>
    </package>
</struts>

